      "routing_key": "N",
      "arguments": {}
    },
    {
      "source": "outbox",
      "vhost": "/",
      "destination": "auth-service",
      "destination_type": "queue",
      "routing_key": "N",
      "arguments": {}
    },
    {
      "source": "outbox",
      "vhost": "/",
      "destination": "tasks-service",
      "destination_type": "queue",
      "routing_key": "N",
      "arguments": {}
